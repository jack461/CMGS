GM_Log("Test05:parameters handling.");
var str = stralloc();
var stp = stralloc();
var pn = 20;
sprintf(str, "par[%d]: %g", pn, get(`pval, pn));
GM_Log(str);

sprintf(str, "par[%s/%d]: %g", edn(stp, `ChanNbr), get(`pnum, `ChanNbr), get(`pval, `ChanNbr));
GM_Log(str);

var cnt = 1;
set(`pval, `ChanNbr, cnt);
set(`pval, `GenNbr, cnt);
set(`pval, `AutoMode, 1);

loop(9,
    wait(30);
    cnt += 1;
    set(`pval, `ChanNbr, cnt);
    set(`pval, `GenNbr, cnt);
);

wait(60);

loop(9,
    wait(30);
    cnt -= 1;
    set(`pval, `ChanNbr, cnt);
    set(`pval, `GenNbr, cnt);
);

wait(30);
set(`pval, `AutoMode, 0);

wait(40);

GM_Log("Test05 done.");


strfree(str);
strfree(stp);
