// The 10M loops test
// with abs and sin...
var startA = ptime();
var idA = "T89";
var str89 = stralloc();
sprintf(str89, "%s-%d-Start [expect %d]", idA, thread(`idt), 6000000);
GM_Log(str89);

var sA = 0, cA = 0;
var epsA = 0.00001;
var iiiA = 0;
loop (100,
    loop (200,
        loop (300,
            iiiA += 1; cA += epsA;
            sA += abs(sin(cA));
        );
    );
);

sprintf(str89, "%s-Duration:  %.2f s.", idA, ptime() - startA);
GM_Log(str89);
sprintf(str89, "%s-Iterations:  %d", idA, iiiA);
GM_Log(str89);
sprintf(str89, "%s-Result: %f", idA, sA);
GM_Log(str89);

sprintf(str89, "%s-%d-End", idA, thread(`idt));
GM_Log(str89);
strfree(str89);



