GM_Log("T01:factoriel(10)");
function fact(n)
(
   n > 1 ? 
      n * fact(n-1)
   :
      1;
);

var id = "T01";
var str01 = stralloc();
sprintf(str01, "%s-Start [expect: %d]", id, 3628800);
GM_Log(str01);
var start = ptime();

var res = fact (10);

sprintf(str01, "%s-Duration:  %.6f s.", id, ptime() - start); 
GM_Log(str01);

sprintf(str01, "%s-Result is %d", id, res);
GM_Log(str01);

strfree(str01);

