var start = ptime();
var id = "T88";
var str = stralloc();
sprintf(str, "%s-%d-Start", id, thread(`idt));
GM_Log(str);

var s = 0, c = 0;
var eps = 0.00001;
var iii = 0;


loop (100,
    loop (50,
        loop (1000,
            iii += 1; c += eps;
            s += abs(sin(c));
        );
    );
);

sprintf(str, "%s-%d-Phase1 end", id, thread(`idt));
GM_Log(str);

wait(30);


sprintf(str, "%s-%d-Phase2 start", id, thread(`idt));
GM_Log(str);


loop (500,
    loop (10,
        loop (1000,
            iii += 1; c += eps;
            s += abs(sin(c));
        );
    );
);

sprintf(str, "%s-Duration:  %.2f s.", id, ptime() - start);
GM_Log(str);
sprintf(str, "%s-Iterations:  %d", id, iii);
GM_Log(str);
sprintf(str, "%s-Result: %f", id, s);
GM_Log(str);


sprintf(str, "%s-%d-End", id, thread(`idt));
GM_Log(str);

strfree(str);

